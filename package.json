{
  "name": "@astro-masters/vue3-dropzone-ts",
  "description": "Vue simple dropzone",
  "private": false,
  "version": "1.0.0",
  "author": "Stanislav Dremin <st.dremin@ya.ru>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/Vue3Dropzone.umd.js",
  "module": "./dist/Vue3Dropzone.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/Vue3Dropzone.es.js",
      "require": "./dist/Vue3Dropzone.umd.js"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": [
    "dist/style.css"
  ],
  "keywords": [
    "vue",
    "dropzone",
    "drop",
    "zone",
    "input",
    "image",
    "preview"
  ],
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "vite build --config vite.config.ts",
    "preview": "vite preview --config vite.config.ts",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astro-masters/vue3-dropzone.git"
  },
  "bugs": {
    "url": "https://github.com/astro-masters/vue3-dropzone/issues"
  },
  "homepage": "https://github.com/astro-masters/vue3-dropzone#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@types/node": "^22.10.2",
    "@tsconfig/node22": "^22.0.0",
    "@vue/language-core": "^3.2.1",
    "@vue/tsconfig": "^0.7.0",
    "sass-embedded": "^1.83.0",
    "typescript": "^5.7.2",
    "unplugin-dts": "1.0.0-beta.6",
    "vue": "^3.4.19",
    "vue-tsc": "^3.2.1",
    "vite": "^5.1.4"
  },
  "peerDependencies": {
    "vue": "^3.4.19"
  }
}
